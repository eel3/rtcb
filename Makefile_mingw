# @brief   rtcb Makefile for MinGW/TDM-GCC.
# @author  eel3
# @date    2014/06/12
#
# @note
# - TDM-GCC 4.8.1 (64bit) + Cygwin 1.7.28

app        := rtcb
CC         := gcc
CFLAGS     := -Wall -ansi -pedantic -finput-charset=cp932

.PHONY: all
all: $(app)

.PHONY: clean
clean:
	$(RM) $(app)
